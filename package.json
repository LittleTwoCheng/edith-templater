{
  "name": "edith-templater",
  "author": {
    "name": "Ben Cheng(Little Two)",
    "email": "chengsiuyee@gmail.com"
  },
  "description": "A Microsoft Word Templater",
  "electronWebpack": {
    "title": "Edith Templater",
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "BABEL_ENV=development electron-webpack dev",
    "compile": "electron-webpack",
    "build": "electron-builder -m",
    "dist": "yarn compile && yarn build",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "mac": {
      "target": ["dmg","zip"]
    },
    "win": {
      "target": "portable"
    }
  },
  "dependencies": {
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "source-map-support": "^0.5.10",
    "jszip": "^2.6.1",
    "tv4": "^1.3.0",
    "uid-generator": "^1.0.0",
    "docxtemplater": "^3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "electron": "4.0.1",
    "electron-builder": "20.38.4",
    "electron-webpack": "^2.6.2",
    "webpack": "4.28.4"
  }
}
